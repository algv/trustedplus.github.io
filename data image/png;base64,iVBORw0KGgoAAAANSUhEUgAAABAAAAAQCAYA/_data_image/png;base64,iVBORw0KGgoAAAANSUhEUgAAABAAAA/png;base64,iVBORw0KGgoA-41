else if (plp.screens === 'screens-lg') plp.screen = 'lg';
else if (plp.screens === 'screens-xs-sm') {
    if (document.body.clientWidth >= 768) plp.screen = 'sm';
    else if (document.body.clientWidth <= 767) plp.screen = 'xs';
